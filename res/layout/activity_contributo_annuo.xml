<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F0F0F0"
    android:orientation="vertical" >
     <include layout="@layout/public_header"/>
    <LinearLayout 
	    android:layout_width="match_parent"
	    android:layout_height="45dip"
	    android:background="#FFD306"
	    android:gravity="center"
	    android:orientation="horizontal">
	    <TextView 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="7dip"
	        android:textColor="#FFFFFF"
	        android:text="您已资格认证成功，请尽快完成会员费"/>
	     <TextView 
	         android:id="@+id/tex_pay"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:textSize="18dip"
	        android:textColor="#FF0000"
	        android:text="支付"/>
	</LinearLayout>
	    <TextView 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="10dip"
	        android:layout_marginTop="15dip"
	        android:textColor="#9D9D9D"
	        android:text="优势"/>
	<View 
	        android:layout_width="match_parent"
	        android:layout_height="1dip"
	        android:background="#D0D0D0"/>
	<LinearLayout 
	    android:layout_width="match_parent"
	    android:layout_height="40dip"
	    android:background="#FFFFFF"
	    android:gravity="center_vertical"
	    android:orientation="horizontal">
	    <ImageView 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="15dip"
	        android:background="@drawable/imag_high"
	        />
	    <TextView 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="10dip"
	        android:textSize="16dip"
	        android:text="收益"/>
	     <TextView 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="10dip"
	        android:textColor="#842B00"
	        android:textSize="22dip"
	        android:text="高"/>
	      <TextView 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="10dip"
	        android:textSize="16dip"
	        android:text="收益最大可放大4倍"/>
	</LinearLayout>
	<View 
	        android:layout_width="match_parent"
	        android:layout_height="1dip"
	        android:layout_marginLeft="15dip"
	        android:background="#D0D0D0"/>
	<LinearLayout 
	    android:layout_width="match_parent"
	    android:layout_height="40dip"
	    android:background="#FFFFFF"
	    android:gravity="center_vertical"
	    android:orientation="horizontal">
	    <ImageView 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="15dip"
	        android:background="@drawable/imag_fast"
	        />
	    <TextView 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="10dip"
	        android:textSize="16dip"
	        android:text="速度"/>
	     <TextView 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="10dip"
	        android:textColor="#FF0000"
	        android:textSize="22dip"
	        android:text="快"/>
	      <TextView 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="10dip"
	        android:textSize="16dip"
	        android:text="即时申请，即时交易"/>
	</LinearLayout>
	<View 
	        android:layout_width="match_parent"
	        android:layout_height="1dip"
	        android:layout_marginLeft="15dip"
	        android:background="#D0D0D0"/>
	<LinearLayout 
	    android:layout_width="match_parent"
	    android:layout_height="40dip"
	    android:background="#FFFFFF"
	    android:gravity="center_vertical"
	    android:orientation="horizontal">
	    <ImageView 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="15dip"
	        android:background="@drawable/imag_little"
	        />
	    <TextView 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="10dip"
	        android:textSize="16dip"
	        android:text="利息"/>
	     <TextView 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="10dip"
	        android:textColor="#0080FF"
	        android:textSize="22dip"
	        android:text="少"/>
	      <TextView 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="10dip"
	        android:textSize="16dip"
	        android:text="月息仅0.48%"/>
	</LinearLayout>
	<View 
	        android:layout_width="match_parent"
	        android:layout_height="1dip"
	        android:layout_marginLeft="15dip"
	        android:background="#D0D0D0"/>
	<LinearLayout 
	    android:layout_width="match_parent"
	    android:layout_height="40dip"
	    android:background="#FFFFFF"
	    android:gravity="center_vertical"
	    android:orientation="horizontal">
	    <ImageView 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="15dip"
	        android:background="@drawable/imag_low"
	        />
	    <TextView 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="10dip"
	        android:textSize="16dip"
	        android:text="门槛"/>
	     <TextView 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="10dip"
	        android:textColor="#FFD306"
	        android:textSize="22dip"
	        android:text="低"/>
	      <TextView 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="10dip"
	        android:textSize="16dip"
	        android:text="1万元即可申请"/>
	</LinearLayout>
	<View 
	        android:layout_width="match_parent"
	        android:layout_height="1dip"
	        android:background="#D0D0D0"/>
	<View 
	        android:layout_width="match_parent"
	        android:layout_marginTop="20dip"
	        android:layout_height="1dip"
	        android:background="#D0D0D0"/>
	<LinearLayout 
	    android:id="@+id/te_phone_call"
	    android:layout_width="match_parent"
	    android:layout_height="40dip"
	    android:background="#FFFFFF"
	    android:gravity="center"
	    android:orientation="horizontal">
	    <TextView 
	        android:layout_weight="1"
	        android:layout_width="0dip"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="15dip"
	        android:textSize="18dip"
	        android:text="会员服务热线"/>
	    <TextView 
	        android:layout_weight="1"
	        android:layout_width="0dip"
	        android:layout_height="wrap_content"
	        android:gravity="right"
	        android:layout_marginRight="8dip"
	        android:textSize="14dip"
	        android:text="010-56236258"/>
	    <ImageView 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:src="@drawable/raw2right"
	        android:layout_marginRight="10dip"/>
	</LinearLayout>
	<View 
	        android:layout_width="match_parent"
	        android:layout_height="1dip"
	        android:layout_marginLeft="15dip"
	        android:background="#D0D0D0"/>
	<LinearLayout 
	    android:id="@+id/te_admittance"
	    android:layout_width="match_parent"
	    android:layout_height="40dip"
	    android:background="#FFFFFF"
	    android:gravity="center"
	    android:orientation="horizontal">
	    <TextView 
	        android:layout_weight="1"
	        android:layout_width="0dip"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="15dip"
	        android:textSize="18dip"
	        android:text="会员准入条件"/>
	    <TextView 
	        android:layout_weight="1"
	        android:layout_width="0dip"
	        android:layout_height="wrap_content"
	        android:gravity="right"
	        android:layout_marginRight="8dip"
	        android:textSize="14dip"
	        android:text=""/>
	    <ImageView 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:src="@drawable/raw2right"
	        android:layout_marginRight="10dip"/>
	</LinearLayout>
	<View 
	        android:layout_width="match_parent"
	        android:layout_height="1dip"
	        android:layout_marginLeft="15dip"
	        android:background="#D0D0D0"/>
	<LinearLayout 
	    android:id="@+id/te_notice"
	    android:layout_width="match_parent"
	    android:layout_height="40dip"
	    android:background="#FFFFFF"
	    android:gravity="center"
	    android:orientation="horizontal">
	    <TextView 
	        android:layout_weight="1"
	        android:layout_width="0dip"
	        android:layout_height="wrap_content"
	        android:layout_marginLeft="15dip"
	        android:textSize="18dip"
	        android:text="配资须知"/>
	    <TextView 
	        android:layout_weight="1"
	        android:layout_width="0dip"
	        android:layout_height="wrap_content"
	        android:gravity="right"
	        android:layout_marginRight="8dip"
	        android:textSize="14dip"
	        android:text=""/>
	    <ImageView 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:src="@drawable/raw2right"
	        android:layout_marginRight="10dip"/>
	</LinearLayout>
	<View 
	        android:layout_width="match_parent"
	        android:layout_height="1dip"
	        android:background="#D0D0D0"/>
</LinearLayout>
